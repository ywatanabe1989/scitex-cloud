{
  "indent": 4,
  "blank_line_after_tag": "load,extends",
  "close_void_tags": true,
  "format_css": true,
  "format_js": true,
  "ignore": "H006,H030,H031",
  "include": "H017,H035",
  "indent_css": 4,
  "indent_js": 4,
  "max_line_length": 120,
  "max_attribute_length": 80,
  "use_gitignore": true,
  "linter_output_format": "{filename}:{line}: {message} ({code})"
}
